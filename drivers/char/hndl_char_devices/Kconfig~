#
#HNDL-series charater devices
#

menu "HNDL-series character devices "

config HNDL_DEVICES
	tristate "Wfet devices support"
	---help---
	  If you say Y here (and to one of the following options) and create some
	  character special devices.
	  If unsure, say N.


comment "Product Select"
	depends on HNDL_DEVICES
choice
	prompt "HNDL Product"
	default HNDL_PRODUCT_NETMETER

config HNDL_PRODUCT_NETMETER
	bool "NetMeter"
	depends on HNDL_DEVICES && ARCH_AT91SAM9260 

config HNDL_PRODUCT_METERGATHER
	bool "MeterGather"
	depends on HNDL_DEVICES && ARCH_AT91SAM9260 

config HNDL_PRODUCT_HNTT1800X
	bool "HNTT1800X"
	depends on HNDL_DEVICES && ARCH_AT91SAM9260 

config HNDL_PRODUCT_HNTT1800U
	bool "HNTT1800U"
	depends on HNDL_DEVICES && ARCH_AT91SAM9260 

config HNDL_PRODUCT_HNTT1800SJL
	bool "HNTT1800SJL"
	depends on HNDL_DEVICES && ARCH_AT91SAM9260 
	
config HNDL_PRODUCT_HNTT1800SSC
	bool "HNTT1800SSC"
	depends on HNDL_DEVICES && ARCH_AT91SAM9260

config HNDL_PRODUCT_HNDL900B
	bool "HNDL900B"
	depends on HNDL_DEVICES && ARCH_AT91SAM9260 

config HNDL_PRODUCT_HNDL900FJ
	bool "HNDL900FJ"
	depends on HNDL_DEVICES && ARCH_AT91SAM9260 

config HNDL_PRODUCT_HNDL1000X 
    bool "HNDL1000X"
    depends on HNDL_DEVICES && ARCH_AT91SAM9260	

config HNDL_PRODUCT_HNDL1000SG 
	bool "HNDL1000SG"
	depends on HNDL_DEVICES && ARCH_AT91SAM9260	    

config HNDL_PRODUCT_HNTT1800ESH
	bool "ShangHai HNTT1800ESH"
	depends on HNDL_DEVICES && ARCH_AT91SAM9260 

config HNDL_PRODUCT_HNDL1600U
	bool "HNDL1600U"
	depends on HNDL_DEVICES && ARCH_AT91SAM9260 
	
config HNDL_PRODUCT_HNTT1800SH
	bool "HNTT1800SH"
	depends on HNDL_DEVICES && ARCH_AT91SAM9260 

endchoice


comment "Product Independent Device"
	depends on HNDL_DEVICES

